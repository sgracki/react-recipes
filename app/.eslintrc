{
    "extends": ["react-app", "plugin:jsx-a11y/recommended"],
    "plugins": [
        "react-hooks",
        "jsx-a11y"
    ],
    "rules": {
        "react-hooks/rules-of-hooks": "error",
        "react-hooks/exhaustive-deps": "warn",
        "semi": ["error", "always"],
        "comma-dangle": ["error", "always-multiline"],
        "quotes": ["error", "single"]
    }
}
